{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "source": {
    "include": [
      "src/index.js",
      "src/attract.js",
      "src/distributor.js",
      "src/ofHot.js",
      "src/ofLast.js",
      "src/permeate.js"
    ]
  },
  "plugins": ["plugins/markdown"],
  "markdown": {
    "tags": [],
    "excludeTags": [],
    "hardwrap": true
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "opts": {
    "encoding": "utf8",
    "destination": "docs",
    "recurse": true,
    "template": "node_modules/docdash",
    "readme": "README.md"
  }
}
